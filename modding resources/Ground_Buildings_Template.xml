   <Template Name="Template_Ground_Structure" Description="Base template"><!--#-->

      <!--Prevent this from being builable-->
      <Tech_Level>99</Tech_Level>

      <!--Text-->
      <Text_ID>TEXT_GROUND_BUNKER</Text_ID>
      <Encyclopedia_Text>TEXT_ENCYCLOPEDIA_SECONDARY_OUTPOST</Encyclopedia_Text>
      <MP_Encyclopedia_Text>TEXT_ENCYCLOPEDIA_BUNKER</MP_Encyclopedia_Text>
      <Encyclopedia_Unit_Class>TEXT_ENCYCLOPEDIA_CLASS_STRUCTURE</Encyclopedia_Unit_Class>

      <!--Art-->
      <Icon_Name>i_button_bunker.tga</Icon_Name>
      <Land_Model_Name>EB_LightFactory.alo</Land_Model_Name>
      <Scale_Factor>1.0</Scale_Factor>
      <Select_Box_Scale>50</Select_Box_Scale>
      <Select_Box_Z_Adjust>3.0f</Select_Box_Z_Adjust>
      <Radar_Icon_Name>mini_map_structure.tga</Radar_Icon_Name>
      <Radar_Icon_Size>0.05  0.05</Radar_Icon_Size>

      <!--
      CAPTURE_POINT
      DUMMY_GROUND_STRUCTURE
      HIDE_WHEN_FOGGED
      HINT
      IDLE
      REVEAL
      SELECTABLE
      TACTICAL_SELL
      UNIT_AI
      -->
      <!-- Dev Comment: TACTICAL_UNDER_CONSTRUCTION does not work as a Land_Behavior, since the type->Behaves_Like() fails for some reason when it should not - need to fix -->
      <Behavior>SELECTABLE, DUMMY_GROUND_STRUCTURE</Behavior>

      <!--
      ABILITY_COUNTDOWN
      BASE_SHIELD
      CAPTURE_POINT
      DAMAGE_TRACKING
      DUMMY_GROUND_STRUCTURE
      DUMMY_LAND_BASE_LEVEL_COMPONENT
      EARTHQUAKE_TARGET
      GARRISON_STRUCTURE
      GRAVITY_CONTROL_FIELD
      HIDE_WHEN_FOGGED
      IDLE
      ION_STUN_EFFECT
      LAND_OBSTACLE
      LOBBING_SUPERWEAPON
      POWERED
      REVEAL
      SELECTABLE
      SHIELDED
      SPAWN_SQUADRON
      STUNNABLE
      TARGETING
      TERRAIN_TEXTURE_MODIFICATION
      TURRET
      UNIT_AI
      WEAPON-->
      <LandBehavior>DAMAGE_TRACKING, DUMMY_GROUND_STRUCTURE, DUMMY_LAND_BASE_LEVEL_COMPONENT, EARTHQUAKE_TARGET, HIDE_WHEN_FOGGED, ION_STUN_EFFECT, LAND_OBSTACLE, POWERED, REVEAL, STUNNABLE, TARGETING, UNIT_AI</LandBehavior>

      <Affiliation>Neutral</Affiliation>

      <!--If we have an idle animataion we need to add the respective <LandBehavior> and should enable the tag below-->
      <!--Loop_Idle_Anim_00>Yes</Loop_Idle_Anim_00-->

      <!--Balance-->
      <Tactical_Health>100</Tactical_Health>
      <Autoresolve_Health>100</Autoresolve_Health>
      <AI_Combat_Power>200</AI_Combat_Power>
      <Armor_Type>Armor_Structure</Armor_Type>
      <!--Shield_Points>1000</Shield_Points>
      <Shield_Refresh_Rate>10</Shield_Refresh_Rate-->

      <Build_Cost_Credits>1000</Build_Cost_Credits>
      <Build_Time_Seconds>18</Build_Time_Seconds>
      <Sabotage_Cost_Credits>1000</Sabotage_Cost_Credits><!--Should be build cost * 1.2-->
      <Score_Cost_Credits>6000</Score_Cost_Credits>

      <!--Stuff that can be built at the structure-->
      <Tactical_Buildable_Objects_Campaign></Tactical_Buildable_Objects_Campaign>
      <Tactical_Buildable_Objects_Multiplayer></Tactical_Buildable_Objects_Multiplayer>

      <Destruction_Survivors> </Destruction_Survivors>
      <!--Minimum_Destruction_Survivor_Count>4</Minimum_Destruction_Survivor_Count>
		<Maximum_Destruction_Survivor_Count>8</Maximum_Destruction_Survivor_Count-->

      <!--================== Values below this point don't usually need to be changed ==================-->

      <!--1: The unit is available for the player without research-->
      <Build_Initially_Locked>No</Build_Initially_Locked>

      <!--1: -1=infinite. Some buildings should get a production boost, see barracks. TODO=========================================-->
      <Build_Max_Instances_Per_Planet>-1</Build_Max_Instances_Per_Planet>

      <!--1: Units will always fire at the root bone of the building If this is set higher, hostiles will target it this much above the root bone unless there are <Target_Bones> specified.-->
      <Ranged_Target_Z_Adjust>20.0</Ranged_Target_Z_Adjust>

      <!--rebel tech system which we don't use-->
      <Build_Can_Be_Unlocked_By_Slicer>No</Build_Can_Be_Unlocked_By_Slicer>
      <Slice_Cost_Credits>1</Slice_Cost_Credits>

      <!--On Death-->
      <Death_Explosions>Large_Explosion_Land</Death_Explosions>
      <Death_SFXEvent_Start_Die>Unit_Generic_Building_Death_SFX</Death_SFXEvent_Start_Die>

      <!--Standard buildings are available in the lower row-->
      <GUI_Row>0</GUI_Row>

      <!--Sometimes yes, sometimes No, mhhhh-->
      <Is_Dummy>Yes</Is_Dummy>

      <!--Audio-->
      <SFXEvent_Build_Started></SFXEvent_Build_Started>
      <SFXEvent_Build_Complete></SFXEvent_Build_Complete>
      <SFXEvent_Build_Cancelled></SFXEvent_Build_Cancelled>
      <SFXEvent_Unit_Lost></SFXEvent_Unit_Lost>
      <SFXEvent_Unit_Under_Attack></SFXEvent_Unit_Under_Attack>
      <SFXEvent_Ambient_Loop>Structure_Barracks_Loop</SFXEvent_Ambient_Loop>

      <!--Requirements-->
      <Required_Ground_Base_Level>1</Required_Ground_Base_Level><!--2: The ground base level is defined through the number of player buildings I think. This tag does NOT derive to variants!-->
      <Required_Timeline>0</Required_Timeline> <!--2: I think that this tag is deprectated-->
      <Required_Planets /> <!--1: Can be used multiple times, doesn't overwrite-->
      <Required_Special_Structures /><!--1: Does also count for skirmish. Can be used multiple times, doesn't overwrite-->

      <!--Default tags that are set for all of our base buildings-->
      <Mass>1.0</Mass>
      <CategoryMask>Structure</CategoryMask>
      <Size_Value>50</Size_Value>
      <Collidable_By_Projectile_Living>Yes</Collidable_By_Projectile_Living>
      <Build_Tab_Special_Structures>Yes</Build_Tab_Special_Structures>
      <Can_Contain_Heroes_During_Ground_Battle>Yes</Can_Contain_Heroes_During_Ground_Battle>
      <Influences_Capture_Point>False</Influences_Capture_Point>
      <Political_Control>0</Political_Control>
      <Energy_Capacity>10000</Energy_Capacity>
      <Energy_Refresh_Rate>999</Energy_Refresh_Rate>
      <Reveal_During_Setup_Phase>true</Reveal_During_Setup_Phase>
      <Victory_Relevant>yes</Victory_Relevant>
      <Has_Land_Evaluator>True</Has_Land_Evaluator>

      <!--FOW and Radar-->
      <Is_Visible_On_Radar>Yes</Is_Visible_On_Radar>
      <Radar_Show_Facing>No</Radar_Show_Facing>
      <Land_FOW_Reveal_Range>350.0</Land_FOW_Reveal_Range>
      <Multisample_FOW_Check>Yes</Multisample_FOW_Check>
      <Last_State_Visible_Under_FOW>Yes</Last_State_Visible_Under_FOW>
      <Initial_State_Visible_Under_FOW>False</Initial_State_Visible_Under_FOW>

      <!--graphics-->
      <No_Reflection_Below_Detail_Level>1</No_Reflection_Below_Detail_Level>
      <No_Refraction_Below_Detail_Level>1</No_Refraction_Below_Detail_Level>
      <Blob_Shadow_Below_Detail_Level>2</Blob_Shadow_Below_Detail_Level>
      <Blob_Shadow_Material_Name>Square_Shadow</Blob_Shadow_Material_Name>
      <Blob_Shadow_Scale>25.0, 25.0</Blob_Shadow_Scale>

      <!--GUI stuff-->
      <GUI_Bounds_Scale>0.3</GUI_Bounds_Scale>
      <GUI_Bracket_Size>1</GUI_Bracket_Size>
      <!--GUI_Bracket_Height>0</GUI_Bracket_Height>
      <GUI_Bracket_Width>40</GUI_Bracket_Width>
      <GUI_Distance>1000</GUI_Distance>
      <GUI_Model_Name>W_CANTINATAT.ALO</GUI_Model_Name>
      <GUI_Offset>0 0 0</GUI_Offset>
      <GUI_Velocity>45</GUI_Velocity>
      <GUI_X_Rot>90</GUI_X_Rot-->
   </Template>


   <Template Name="Template_Ground_Structure_Big" Description="Used for all kind of buildings which are more than a small bp turret"><!--#-->
      <Variant_Of_Existing_Type>Template_Ground_Structure</Variant_Of_Existing_Type>
      <Death_Explosions>Huge_Explosion_Land</Death_Explosions>
      <GUI_Bracket_Size>2</GUI_Bracket_Size>

      <!--Set tgis to troe for the really big stuff-->
      <Exclude_From_Distance_Fade>false</Exclude_From_Distance_Fade>

      <!-- The ground structures have this data which looks like it's space only because the
      snowspeeders are using space style pathfinding which needs this information so that we won't
      path speeders through structures -->
      <Space_Obstacle_Offset> 0 0 0 </Space_Obstacle_Offset>
      <Space_Layer> Corvette </Space_Layer>

      <!--Check the damage stages (ALT model)-->
      <!--Land_Damage_Thresholds>1, 0.66, 0.33, 0</Land_Damage_Thresholds>
		<Land_Damage_Alternates>0, 1, 2, 3</Land_Damage_Alternates>
		<Land_Damage_SFX>null,SFX_Grenade_Detonation, SFX_Grenade_Detonation,null</Land_Damage_SFX-->

      <!--If the strcture was built on a build pad-->
      <!--Obstacle_Proxy_Type> Communications_Array_E_Build_Pad</Obstacle_Proxy_Type-->

      <!--After the structure dies it will leave ruins behind. The tags below make sure that they cannot be walked through-->
      <!--Death_Clone_Is_Obstacle> true </Death_Clone_Is_Obstacle>
		<Obstacle_Width>40.0</Obstacle_Width>
		<Obstacle_Height>40.0</Obstacle_Height>
		<Obstacle_X_Offset>-20.0</Obstacle_X_Offset>
		<Obstacle_Y_Offset>-20.0</Obstacle_Y_Offset-->

      <!--Used to overwrite collision-->
      <!--Custom_Hard_XExtent>26.0</Custom_Hard_XExtent>
      <Custom_Hard_YExtent>26.0</Custom_Hard_YExtent-->

      <!--We can cause the building to spawn a seperate texture around it. Requires TERRAIN_TEXTURE_MODIFICATION behaviour. Not used in TPC-->
      <!--Terrain_Texture_Modifier_Join_Distance>500.0</Terrain_Texture_Modifier_Join_Distance>
      <Terrain_Texture_Modifier_Material>62</Terrain_Texture_Modifier_Material>
      <Terrain_Texture_Modifier_Square>False</Terrain_Texture_Modifier_Square-->
      <Required_Ground_Base_Level>1</Required_Ground_Base_Level>
    </Template>


    <Template Name="Template_Ground_Structure_Big_Base_Building" Description="Used for base buildings in skirmish and GC"><!--#-->
      <Variant_Of_Existing_Type>Template_Ground_Structure_Big</Variant_Of_Existing_Type>

      <!--Deprecated tag from vanilla EaW before ground buildings became movable on GC layer. Alternative: Out_Base-->
      <Base_Position>In_Base</Base_Position>

      <Property_Flags>InBase</Property_Flags>
      <Reveal_During_Setup_Phase> true </Reveal_During_Setup_Phase>
      <Victory_Relevant>yes</Victory_Relevant>
      <Tactical_Health>1500</Tactical_Health>
      <Autoresolve_Health>1500</Autoresolve_Health>
      <AI_Combat_Power>500</AI_Combat_Power>
      <Armor_Type>Armor_Structure</Armor_Type>
      <Build_Cost_Credits>500</Build_Cost_Credits>
      <Build_Time_Seconds>22</Build_Time_Seconds>
      <Score_Cost_Credits>1000</Score_Cost_Credits>

      <!--Only necessary for multiplayer skirmish-->
      <Is_Community_Property>Yes</Is_Community_Property>

      <!--Most likely unused tag that was required when ground base levels where a thing-->
      <Base_Level_Available>1</Base_Level_Available>

      <!--Every building does self heal. Note that for skirmish, an income stream nees to be added.-->
      <Abilities SubObjectList="Yes">
         <Force_Healing_Ability Name="Generic_Base_Structure_Self_Repair">
            <Activation_Style>Skirmish_Automatic</Activation_Style>
            <Heal_Range>0.0</Heal_Range>
            <Heal_Percent>0.025</Heal_Percent>
            <Heal_Amount>0.0</Heal_Amount>
            <Heal_Interval_In_Secs>5.0</Heal_Interval_In_Secs>
            <Single_Target_Heal>Yes</Single_Target_Heal>
            <Applicable_Unit_Categories />
            <Applicable_Unit_Types />
            <SFXEvent_Target_Affected />
            <Owner_Particle_Bone_Name>ROOT</Owner_Particle_Bone_Name>
            <Owner_Light_Effect_Type>Color_Flash</Owner_Light_Effect_Type>
            <Owner_Light_Effect_Color>1.2, 1.2, 1.2</Owner_Light_Effect_Color>
            <Owner_Light_Effect_Color2 />
            <Owner_Light_Effect_Duration>0.6</Owner_Light_Effect_Duration>
            <Owner_Light_Effect_Pulse_Count>1</Owner_Light_Effect_Pulse_Count>
         </Force_Healing_Ability>
      </Abilities>
      <Required_Ground_Base_Level>1</Required_Ground_Base_Level>
   </Template>


